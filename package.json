{
  "name": "urban-express-ts-api",
  "version": "1.0.0",
  "description": "",
  "main": "./dist/index.js",
  "scripts": {
    "build": "tsc --build tsconfig.json",
    "start": "node .",
    "dev": "npx ts-node ./src/index.ts",
    "dev2": "npx ts-node ./test/app.ts",
    "watch": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "test": "ocha -r ts-node/register test/**/*.spec.ts",
    "test-dev": "nodemon --watch . --ext ts --exec \"mocha -r ts-node/register test/**/*.spec.ts\"",
    "start:mongo": "docker run -p 27017:27017 --name urban_mongo_ds -d mongo:latest",
    "start:redis": "docker run -p 6379:6379 --name urban_redis_ds -d redis:latest"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "^4.2.14",
    "@types/chai-http": "^4.2.0",
    "@types/cors": "^2.8.8",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.9",
    "@types/helmet": "^4.0.0",
    "@types/mocha": "^8.0.4",
    "@types/mongoose": "^5.10.1",
    "@types/node": "^14.14.10",
    "@types/node-geocoder": "^3.24.1",
    "@types/point-in-polygon": "^1.0.0",
    "@types/redis": "^2.8.28",
    "@types/sinon": "^9.0.9",
    "@types/supertest": "^2.0.10",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "mocha": "^8.2.1",
    "nodemon": "^2.0.6",
    "sinon": "^9.2.1",
    "supertest": "^6.0.1",
    "ts-node": "^9.0.0",
    "tslint": "^6.1.3",
    "typescript": "^4.1.2"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^4.2.0",
    "mongoose": "^5.10.17",
    "node-geocoder": "^3.27.0",
    "point-in-polygon": "^1.0.1",
    "redis": "^3.0.2"
  }
}
